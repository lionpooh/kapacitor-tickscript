var id string
var message string
var measurement string

var groups = [*]
var period duration
var every duration

var info lambda
var warn lambda
var crit lambda

var path string

var post string

var data = stream 
		|from()
		 .measurement(measurement)
		 .groupBy(groups)
		|window()
		 .period(period)
		 .every(every)
var alert = data
		|alert()
		 .id(id)
		 .message(message)
		 .details('')
		 .info(info)
		 .warn(warn)
		 .crit(crit)
		 .log(path)
		//.post(post)
